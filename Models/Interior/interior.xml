<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

<animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>100</max-m>
    </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
     <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>GLASS-Canopy-R-Glass</object-name>
       <object-name>GLASS-FixedGlass</object-name>
	    <object-name>HUDGlass</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>GLASS-Canopy-R-Glass</object-name>
       <object-name>GLASS-FixedGlass</object-name>
	    <object-name>HUDGlass</object-name>
  </animation>

  <effect>
       <inherits-from>Effects/model-transparent</inherits-from>
       <object-name>GLASS-Canopy-R-Glass</object-name>
       <object-name>GLASS-FixedGlass</object-name>
	    <object-name>HUDGlass</object-name>
   </effect>
  
  
  <!-- Dash-->
 
<model>
    <path>Aircraft/F-23C/Models/Interior/interior.ac</path>
    <offsets>
      <x-m> 0 </x-m>
      <y-m> -0.0 </y-m>
      <z-m> 0.00 </z-m>
    </offsets>
  </model>
  
  <model>
    <path>Aircraft/F-23C/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -1.7 </x-m>
      <y-m> 0.05</y-m>
      <z-m> 0.00 </z-m>
    </offsets>
  </model>







</PropertyList>

