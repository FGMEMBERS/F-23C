<?xml version="1.0"?>



<PropertyList>
	<path>A-10-an-arc-186.ac</path>

	<animation>
		<type>select</type>
		<object-name>25</object-name>
		<object-name>50</object-name>
		<object-name>75</object-name>
		<condition>
			<property>sim/model/Hawk/dummy-node</property>
		</condition>
	</animation>

<!-- lighting-->

	<animation>
		<object-name>tone-sqdis-switch</object-name> 
		<object-name>pressets-wheel</object-name> 
		<object-name>vol-knob</object-name> 
		<object-name>freq-knob-10</object-name> 
		<object-name>freq-knob-1</object-name> 
		<object-name>freq-knob-01</object-name> 
		<object-name>freq-knob-0001</object-name> 
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.35</red>
			<green>0.24</green>
			<blue>0.14</blue>
		</emission>
	</animation>

	<animation>
		<object-name>freq-select-knob</object-name> 
		<object-name>load-button</object-name> 
		<object-name>mode-knob</object-name> 
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.35</red>
			<green>0.24</green>
			<blue>0.14</blue>
		</emission>
	</animation>

	<animation>
		<object-name>dot</object-name> 
		<object-name>ring-01</object-name> 
		<object-name>ring-1</object-name> 
		<object-name>ring-10</object-name> 
		<object-name>pressets-01</object-name> 
		<object-name>pressets-10</object-name> 
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.75</red>
			<green>0.55</green>
			<blue>0.35</blue>
		</emission>
	</animation>

	<animation>
		<object-name>mount</object-name> 
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.49</red>
			<green>0.35</green>
			<blue>0.25</blue>
		</emission>
	</animation>

	<animation>
		<object-name>00</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
			<red>0.75</red>
			<green>0.55</green>
			<blue>0.35</blue>
		</emission>
	</animation>

	<animation>
		<object-name>uhf</object-name>
		<type>noshadow</type>
	</animation>

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
