<?xml version="1.0"?>

<PropertyList>

  <path>vii1.ac</path>

 <animation>
  <type>noshadow</type>
  <object-name>indaltlmob.001</object-name>
  <object-name>indalti.001</object-name>
 </animation>

 <animation> <!-- link electrical system -->
  <type>select</type>
  <object-name>indaltlmob.001</object-name>
  <object-name>indalti.001</object-name>
	 <condition>
		<greater-than>
			 <property>systems/electrical/outputs/avionics/ASI</property>
			 <value>22</value>
		</greater-than>
	 </condition>
 </animation>



	<animation> <!-- fixed show on 2.0 -->
	  <object-name>indaltlmob.001</object-name>
	  <object-name>indalti.001</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/ULP/brightness</factor-prop>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.5</blue>
		</emission>
	</animation>



  <animation>
    <type>translate</type>
    <object-name>indaltlmob.001</object-name>
    <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
    <!--factor>0.1</factor>
	<step>1</step-->
	<z-min>-0.0233</z-min>
	<z-max>0.0233</z-max>
    <interpolation>
      <entry><ind>-1000000 </ind><dep> -0.02290 </dep></entry>
      <entry><ind>  -10000 </ind><dep> -0.02240 </dep></entry>
      <entry><ind>   -2000 </ind><dep> -0.01500 </dep></entry>
      <entry><ind>   -1500 </ind><dep> -0.01130 </dep></entry>
      <entry><ind>   -1000 </ind><dep> -0.00758 </dep></entry>
      <entry><ind>    -500 </ind><dep> -0.00381 </dep></entry>
      <entry><ind>       0 </ind><dep>  0.00000 </dep></entry>
      <entry><ind>     500 </ind><dep>  0.00381 </dep></entry>
      <entry><ind>    1000 </ind><dep>  0.00758 </dep></entry>
      <entry><ind>    1500 </ind><dep>  0.01130 </dep></entry>
      <entry><ind>    2000 </ind><dep>  0.01500 </dep></entry>
      <entry><ind>   10000 </ind><dep>  0.02240 </dep></entry>
      <entry><ind> 1000000 </ind><dep>  0.02290 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>

</PropertyList>

