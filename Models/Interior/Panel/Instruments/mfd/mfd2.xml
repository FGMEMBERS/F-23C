    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>-0.034</y-m>
      <z-m>-0.078</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">ENG</text>

	</text>
	<text>
	 <name>hydraulic</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
      <x-m>0.018</x-m>
      <y-m>-0.01</y-m>
      <z-m>-0.078</z-m>
    </offsets>
	  <type type="string">literal</type>
	  <text type="string">HYD</text>
	</text>
	<text>
	 <name>waypoint</name>
	  <font>lcd.txf</font>
    <character-size type="double">0.013</character-size>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <heading-deg>0</heading-deg>
      <roll-deg>90</roll-deg>
